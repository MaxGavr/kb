			-> rrel_2: rrel_assign: rrel_scp_var: rrel_pos_const_perm: ...;;
			-> rrel_3: rrel_assign: rrel_scp_var: .._current_year;;

			=> nrel_then: ..proc_operator_compare_current_year_with_min_year_equality;;
			=> nrel_else: ..proc_operator_get_book_from_set;;
		*);;

		-> ..proc_operator_compare_current_year_with_min_year_equality (*
