			-> rrel_1: rrel_fixed: rrel_scp_var: .._current_year;;
			-> rrel_2: rrel_fixed: rrel_scp_var: .._min_year;;
	
			=> nrel_then: ..proc_operator_add_book_to_answer;;
			=> nrel_else: ..proc_operator_compare_current_year_with_max_year_equality;;
		*);;

		-> ..proc_operator_compare_current_year_with_max_year_equality (*
