	*);;

	-> rrel_operators: ... (*

		-> rrel_init: ..proc_operator_generate_answer (*
			<- genEl;;

			-> rrel_1: rrel_assign: rrel_scp_var: rrel_node: .._found_books;;

			=> nrel_goto: ..proc_operator_find_all_books;;
		*);;

		-> ..proc_operator_find_all_books (*
			<- searchSetStr3;;

			-> rrel_1: rrel_fixed: rrel_scp_const: book;;
			-> rrel_2: rrel_assign: rrel_scp_var: rrel_pos_const_perm: ...;;
			-> rrel_3: rrel_assign: rrel_scp_var: rrel_node: ...;;

