			-> rrel_2: rrel_assign: rrel_scp_var: rrel_pos_const_perm: .._book_set_arc;;
			-> rrel_3: rrel_assign: rrel_scp_var: rrel_node: .._current_book;;

			=> nrel_then: ..proc_operator_delete_current_book_from_set;;
		 	=> nrel_else: ..proc_operator_return;;
		*);;

		-> ..proc_operator_delete_current_book_from_set (*
			<- eraseEl;;

			-> rrel_1: rrel_fixed: rrel_scp_var: rrel_erase: .._book_set_arc;;

			=> nrel_goto: ..proc_operator_find_publishing_year;;
		*);;

		 -> ..proc_operator_find_publishing_year (* 
			<- searchElStr5;;

			-> rrel_1: rrel_fixed: rrel_scp_var: .._current_book;;
			-> rrel_2: rrel_assign: rrel_scp_var: ...;;
			-> rrel_3: rrel_assign: rrel_scp_var: .._current_publish_year;;
			-> rrel_4: rrel_assign: rrel_scp_var: rrel_pos_const_perm: ...;;
			-> rrel_5: rrel_fixed: rrel_scp_const: nrel_publication_date;;
	
			=> nrel_then: ..proc_operator_get_link_with_year;;
			=> nrel_else: ..proc_operator_get_book_from_set;;
		 *);;

		-> ..proc_operator_get_link_with_year (*
