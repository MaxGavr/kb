sc_agent_of_search_book_by_time_period
=> nrel_main_idtf:
	[sc-агент поиска книг, изданных в указанный период]
		(* <- lang_ru;; *);
	[sc-agent of search for books, published in certain period]
		(* <- lang_en;; *);

<- abstract_sc_agent;
=> nrel_primary_initiation_condition:
	(sc_event_add_output_arc => question_initiated);
=> nrel_initiation_condition_and_result:
	(..sc_agent_of_search_book_by_time_period_initiation_condition => ..sc_agent_of_search_book_by_time_period_result);
<= nrel_sc_agent_key_sc_elements: 
	{
		question_initiated;
		question;
		question_search_book_by_time_period
	};
=> nrel_inclusion: ...
	(*
		<- platform_independent_abstract_sc_agent;;

		<= nrel_sc_agent_program: 
		{
		agent_proc_search_book_by_time_period;
		proc_search_book_by_time_period
		};;
	
		-> sc_agent_of_search_book_by_time_period_scp
			(* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_search_book_by_time_period_initiation_condition
= [*
	question_search_book_by_time_period _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_of_search_book_by_time_period_result
= [*
	question_search_book_by_time_period _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;
